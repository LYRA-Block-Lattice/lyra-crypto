{
  "compilerOptions": {
    "target": "es2020",
    "module": "commonjs",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "lib": ["dom", "es6", "es2017"],
    //"types": ["node"],
    "declaration": true,
    "outDir": "lib",
    "rootDir": "src",
    "strict": true,
    "strictPropertyInitialization": false
  },
  "exclude": ["lib", "node_modules"],
  "include": ["src"]
}
